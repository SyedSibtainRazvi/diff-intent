{
  "name": "diff-intent",
  "version": "1.0.0",
  "description": "AI-powered git diff analysis - understand the intent behind code changes",
  "main": "dist/index.js",
  "bin": {
    "diff-intent": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "format": "biome format --write src/",
    "check": "biome check --write src/ && tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "pnpm run check && pnpm run build"
  },
  "keywords": [
    "git",
    "diff",
    "ai",
    "llm",
    "code-review",
    "developer-tools",
    "cli",
    "groq",
    "openai",
    "anthropic",
    "claude"
  ],
  "author": "Syed Sibtain Razvi <syedsibtain191@gmail.com>",
  "license": "MIT",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SyedSibtainRazvi/diff-intent.git"
  },
  "bugs": {
    "url": "https://github.com/SyedSibtainRazvi/diff-intent/issues"
  },
  "homepage": "https://github.com/SyedSibtainRazvi/diff-intent#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.28.1",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "commander": "^14.0.2",
    "cosmiconfig": "^9.0.0",
    "diff": "^5.2.0",
    "dotenv": "^17.2.3",
    "gpt-tokenizer": "^2.1.2",
    "inquirer": "^9.2.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@types/diff": "^5.2.0",
    "@types/inquirer": "^9.0.0",
    "@types/node": "^25.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
